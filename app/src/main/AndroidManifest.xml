<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.gym_app">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gym_app"
        tools:targetApi="31">
        <activity
            android:name=".Progress"
            android:exported="false" />
        <activity
            android:name=".cal_output"
            android:exported="false"
            />
        <activity
            android:name=".About"
            android:exported="false" />
        <activity
            android:name=".Register"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".login"
            android:exported="true"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".calculate"
            android:exported="false"
            android:noHistory="true"/>
        <activity
            android:name=".Ex_video"
            android:exported="false"
            android:label="@string/title_activity_scrolling_ex1_video"
            android:parentActivityName=".Exercise"
            android:theme="@style/Theme.Gym_app.NoActionBar" />
        <activity
            android:name=".Exercise"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"/>
    </application>

</manifest>